{
	"manifest_version": 2,
	"name": "СкачивателЬ",
	"version": "0.0.2",
	"description": "Скачивание заявок",
	"browser_action": {
		"default_icon": "./icons/downloads-icon.png",
		"default_title": "СкачивателЬ",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "./icons/downloads-icon.png",
		"128": "./icons/downloads-icon.png"
	},
	"permissions": [		
		"<all_urls>",
		"storage",
		"tabs",
		"activeTab"
	]
}